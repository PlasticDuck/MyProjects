#ifndef MESHGRIDSAVER_H_
#define MESHGRIDSAVER_H_
#include <string>

class MeshGridSaver {
private:
	std::string destination;
	std::string fileName;
	double (*function)(std::vector<double>);
	double *intervals;
public:
	MeshGridSaver(double (*function)(std::vector<double>),double*,std::string,std::string);
	virtual ~MeshGridSaver();
};

#endif /* MESHGRIDSAVER_H_ */
