#include "PyRunner.h"

PyRunner::PyRunner(char *argv) {
	this->argv=argv;
	this->main=NULL;
}

PyRunner::~PyRunner() {
	// TODO Auto-generated destructor stub
}

PyObject* PyRunner::getMainPyObject(){
	return this->main;
}

void PyRunner::star(){
	Py_SetProgramName("Drawer");
	Py_Initialize();
	this->main=PyImport_AddModule("__main__");
}

void PyRunner::end(){

}
