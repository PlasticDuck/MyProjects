#include "EvolutionCollector.h"

EvolutionCollecter::EvolutionCollecter() {
	this->collection=new std::vector<std::vector<double> >();
}

EvolutionCollecter::~EvolutionCollecter() {
	// TODO Auto-generated destructor stub
}

void EvolutionCollecter::add(std::vector<double> evolution){
	this->collection->push_back(evolution);
}

