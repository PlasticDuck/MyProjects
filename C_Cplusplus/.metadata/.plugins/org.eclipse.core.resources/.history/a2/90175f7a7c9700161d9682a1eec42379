#include "EvolutionCollecter.h"

EvoluationCollecter::EvoluationCollecter() {
	this->collection=new std::vector<std::vector<double> >();
}

EvoluationCollecter::~EvoluationCollecter() {
	// TODO Auto-generated destructor stub
}

void EvoluationCollecter::add(std::vector<double> evolution){
	this->collection->push_back(evolution);
}

