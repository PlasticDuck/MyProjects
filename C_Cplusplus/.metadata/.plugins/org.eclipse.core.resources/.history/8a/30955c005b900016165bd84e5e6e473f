#include <iostream>
#include <vector>
#include <array>
#include <stdio.h>
//My includes files
#include "Bacteria.h"
#include "Population.h"


using namespace std;

int main() {
	cout << "!!!START!!!" << endl;
	vector<double> x;
	x.push_back(1.0);x.push_back(1.6);x.push_back(0.1);
	double step=0.1;
	Bacteria bac(x,step);
	bac.printBacteria();


	vector<double[2]> intervals;
	intervals.push_back({-10.0,10.0});
	intervals.push_back({-10.0,10.0});
	Population pop(10,intervals);
	pop.printIntervals();

	cout << "!!!END!!!" << endl;
	return 0;
}
