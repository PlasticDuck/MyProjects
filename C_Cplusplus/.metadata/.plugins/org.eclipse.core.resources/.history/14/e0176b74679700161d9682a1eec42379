import matplotlib.pyplot


def drawEvolution(meanOfPopulation):
    indexes=range(1,len(meanOfPopulation)+1)
    matplotlib.pyplot.hold(True);
    matplotlib.pyplot.plot(indexes,meanOfPopulation)
    matplotlib.pyplot.xlabel("Chemotaxis index");
    matplotlib.pyplot.ylabel("Mean value (log10)");
