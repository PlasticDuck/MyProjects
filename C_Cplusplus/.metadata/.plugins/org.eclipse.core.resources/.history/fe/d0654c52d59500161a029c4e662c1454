#include <iostream>
#include <jansson.h>
using namespace std;

int main() {
	json_t *ob;
	int *r=new int[3]{1,2,3};
	ob=json_pack("[fmt]",r);
	char* ss=json_dumps(ob)
	cout << "Testeles"<< << endl; // prints Testeles
	return 0;
}
